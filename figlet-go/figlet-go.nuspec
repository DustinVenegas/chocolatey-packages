<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>figlet-go</id>
    <title>FIGlet-go (Portable)</title>
    <version>1.0.0</version>
    <authors>Luke Sampson</authors>
    <owners>Dustin Venegas</owners>
    <summary>FIGlet Go is a port that makes large letters out of ordinary text</summary>
    <description>
        FIGlet Go is a port from the C version of FIGlet. It makes large ASCII art text out of ordinary text. 

        The Unix version of FIGlet is located at http://www.figlet.org/.
    </description>
    <projectUrl>https://github.com/lukesampson/figlet</projectUrl>
    <tags>console ascii-art</tags>
    <copyright>2013</copyright>
    <licenseUrl>http://opensource.org/licenses/BSD-3-Clause</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <releaseNotes></releaseNotes>
  </metadata>
  <files>
    <file src="tools\**" target="tools" />
  </files>
</package>
